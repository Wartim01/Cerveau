{
  "manifest_version": 3,
  "name": "DATALoL",
  "version": "1.0.0",
  "description": "Une extension Chrome conçue pour extraire et analyser les données de https://gol.gg/, répondant aux demandes dynamiques de 'Cerveau'.",
  "permissions": [
    "activeTab",
    "storage",
    "https://gol.gg/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://gol.gg/*"],
      "js": ["contentScript.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "DATALoL",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*", "styles/*"],
      "matches": ["https://gol.gg/*"]
    }
  ]
}